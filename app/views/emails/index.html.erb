<h1>Emails#index</h1>

<div class="data_container">

<h3>Total number of emails: <%= @emails.count %> </h3>

</div>

<div class="data_container">

<h3>Total number of emails open: <%= @emails_opened %> </h3>

</div>

<div class="data_container">

<h3>Total number of clicks: <%= @emails_clicked %> </h3>

</div>

<div class="data_container">

<% @click_rates_array.each do |click_rate_hash| %>

</div>

<div class="data_container">

	<h3><%= click_rate_hash['email_type'] %> click rate: <%= click_rate_hash['click_rate'] %>%</h3>
</div>

<% end %>



